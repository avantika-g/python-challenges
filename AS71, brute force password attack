import random

charas = ["!","#","$","%","&","'","*","+",",","-",".","/",":",";","=","[","]","^","_",'1','2','3','4','5','6','7','8','9','0','a','e','i','o','u','b','c','d,','e','f','g','h','i','j','k']

pword = ''
for i in range(8):
    pword += random.choice(charas)

print(pword)
pword2 = ''
while pword2 != pword:
    pword2 = ''
    for i in range(8):
        pword2 += random.choice(charas)
print(pword2)
