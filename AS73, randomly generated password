import random

color = input("What is your favourite color? ")
place = input("What is your favourite place? ")
animal = input("What is your favourite animal? ")
symbols = ['!','@','#','$','%','^','*','(','+']
pword = []

for i in range (int((len(color))/2)):
    pword.append(color[i])

for i in range (int((len(place))/2)):
    pword.append(place[i])

for i in range (int((len(animal))/2)):
    pword.append(animal[i])

for i in range (3):
    pword.append (random.choice(symbols))

print (pword)
