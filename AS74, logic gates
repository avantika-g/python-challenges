g = input("What gate do you want to make? ")

if g == "and":
    a = int(input("set a to 1 or 0? "))
    b = int(input("set b to 1 or 0? "))
    if a == 1 and b == 1:
        print("output is 1")
    else:
        print("output is 0")
if g == "or":
    a = int(input("set a to 1 or 0? "))
    b = int(input("set b to 1 or 0? "))
    if a == 0 or b == 0:
        print("output is 0")
    else:
        print("output is 1")
if g == "nor":
    a = int(input("set a to 1 or 0? "))
    b = int(input("set b to 1 or 0? "))
    if a == 0 and b == 0:
        print("output is 1")
    else:
        print("output is 0")
if g == "xor":
    a = int(input("set a to 1 or 0? "))
    b = int(input("set b to 1 or 0? "))
    if a == 1 or b == 1:
        print("output is 1")
    if a == 0 and b == 0:
        print("output is 0")
    if a ==1 or b ==1:
        print("output is 0")
if g == "not":
    a = int(input("set a to 1 or 0? "))
    if a == 1:
        print("output is 0")
    else:
        print("output is 1")
